                </Button>
                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="160.0" text="\$300" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="140.0" text="\$200" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="\$100" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="5" GridPane.valignment="CENTER">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
            </children>
        </GridPane>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#handleBackButton" prefHeight="60.0" prefWidth="275.0" styleClass="exit-button" text="Menu" textAlignment="CENTER">
            <font>
                <Font size="24.0" />
            </font>
            <VBox.margin>
                <Insets bottom="50.0" left="50.0" right="50.0" top="20.0" />
            </VBox.margin>
        </Button>
        <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="NEVER">
            <children>
                <Button alignment="CENTER" maxHeight="66.0" maxWidth="66.0" minHeight="66.0" minWidth="66.0" mnemonicParsing="false" onAction="#handleSettingsButton" prefHeight="66.0" prefWidth="66.0" styleClass="setting-button" textAlignment="CENTER">
