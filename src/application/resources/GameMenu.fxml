                </Button>
                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="140.0" text="\$200" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Button>
                <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0" prefWidth="120.0" text="\$100" GridPane.halignment="CENTER" GridPane.rowIndex="5" GridPane.valignment="CENTER">
