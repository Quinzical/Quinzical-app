                        <Font size="24.0" />
                    </font>
                    <graphic>
                        <ImageView fitHeight="70.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@images/settings.png" />
                            </image>
                            <viewport>
                                <Rectangle2D />
                            </viewport>
                            <effect>
                                <Blend />
                            </effect>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
            <VBox.margin>
